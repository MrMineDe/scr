VOL=$(pulsemixer --get-volume | awk '{print $1}')
MUTE=$(pulsemixer --get-mute)
if [ $MUTE = 1 ]; then
	echo ðŸ”‡$VOL
elif [ $VOL -lt 1 ]; then
	echo ðŸ”‡$VOL
elif [ $VOL -lt 34 ]; then
	echo ðŸ”ˆ$VOL
elif [ $VOL -lt 67 ]; then
	echo ðŸ”‰$VOL
elif [ $VOL -le 100 ]; then
	echo ðŸ”Š$VOL
elif [ $VOL -le 200 ]; then
	echo ðŸ”´$VOL
fi
